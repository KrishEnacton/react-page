{
  "name": "@react-page/plugins-video",
  "version": "0.0.0",
  "main": "./lib-cs/index.js",
  "module": "./lib/index.js",
  "sideEffects": false,
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib-cs/index.js"
    },
    "./lib/index.css": "./lib/index.css"
  },
  "author": "ORY GmbH",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:lib build:css",
    "build:watch": "npm-run-all --parallel build:lib:watch build:css:watch",
    "build:lib": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-cs.json",
    "build:lib:watch": "yarn build:lib -- --watch",
    "build:css": "postcss --config ../../../../config/postcss.config.js --dir lib/ src/index.css",
    "build:css:watch": "npm-run-all build:css -- -w",
    "docs": "documentation build ./lib/**/*.js --format md --github -o ../../../../docs/api/plugins/video.md",
    "clean": "rimraf \"lib-cs\" && rimraf \"lib\" && rm -f *.tsbuildinfo"
  },
  "peerDependencies": {
    "@mui/material": "*",
    "react": ">= 16.14",
    "react-dom": ">= 16.14"
  },
  "dependencies": {
    "@mui/icons-material": "^5.8.0",
    "@react-page/editor": "0.0.0",
    "react-player": "^2.10.1"
  },
  "devDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
